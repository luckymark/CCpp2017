C1


1.
#include<iostream>
using namespace std;
int main()
{
    int a;
    cin>>a;
    if(a>0)
    {
        cout<<a<<" is positive"<<endl;
    }
    else if(a<0)
    {
        cout<<a<<" is negative"<<endl;
    }
    else
    {
        cout<<a<<" is neither positive nor negative"<<endl;
    }
    if(a%2==0)
    {
        cout<<a<<" is even"<<endl;
    }
    else
    {
        cout<<a<<" is odd"<<endl;
    }
    return 0;
}


2.
#include<iostream>
using namespace std;
int main()
{
    int a;
    cin>>a;
    if(a%13==0)
    {
        cout<<a<<" is the multiple of 13"<<endl;
    }
    else
    {
        cout<<a<<" is not the multiple of 13"<<endl;
    }
    return 0;
}


3.
#include<iostream>
using namespace std;
int main()
{
    int a,b=0;
    cin>>a;
    if(a%4==0)
    {
        if(a%100!=0)
        {
            b=1;
        }
        else if(a%400==0)
        {
            b=1;
        }
    }
    if(b==1)
    {
        cout<<a<<" is a leap year"<<endl;
    }
    else
    {
        cout<<a<<" is not a leap year"<<endl;
    }
    return 0;
}


