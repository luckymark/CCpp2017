    {
        cout<<a;
    }
    else
    {
        cout<<b;
    }
    return 0;
}


5.
#include<iostream>
#include<algorithm>
using namespace std;
int main()
{
    int a[3];
    for(int i=0;i<3;i++)
    {
        cin>>a[i];
    }
    sort(a,a+3);
    cout<<a[2];
    return 0;
}


6.
#include<iostream>
#include<algorithm>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    sort(a,a+n);
    cout<<a[n-1];
    return 0;
}



