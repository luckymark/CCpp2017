3.
#include<iostream>
using namespace std;
int main()
{
    int a=0;
    for(int i=7;i<100;i+=7)
    {
        a+=i;
        cout<<i<<endl;
    }
    for(int i=17;i<100;i+=10)
    {
        if(i%7!=0)
        {
            a+=i;
            cout<<i<<endl;
        }
    }
    cout<<"sum: "<<a;
    return 0;
}


4.
#include<iostream>
using namespace std;
int main()
{
    for(int i=1;i<10;i++)
    {
        for(int j=i;j<10;j++)
        {
            cout<<i<<"*"<<j<<"="<<i*j<<endl;
        }
    }
    return 0;
}


5.
#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(int i=n-1;i>=0;i--)
    {
        cout<<a[i]<<endl;
    }
    return 0;
}


6.
#include<iostream>
using namespace std;
int main()
{
    int a[10];
    int b;
    for(int i=0;i<10;i++)
    {
        a[i]=i;
    }
    for(int i=0;i<5;i++)
    {
        cin>>b;
        a[b]=-1;
    }
    for(int i=0;i<10;i++)
    {
        if(a[i]>0)
        {
            cout<<a[i]<<endl;
        }
    }
    return 0;
}


7.
#include<iostream>
using namespace std;
int main()
{
    int b;
    int a[10];
    for(int i=0;i<10;i++)
    {
        a[i]=0;
    }
    for(int i=0;i<5;i++)
    {
        cin>>b;
        a[b]++;
    }
    for(int i=0;i<10;i++)
    {
        for(int j=0;j<a[i];j++)
        {
            cout<<i<<endl;
        }
    }
    return 0;
}



C3


1.
#include<iostream>
using namespace std;
int sum_up(int n,int l[n])
{
    int b=0;
    for(int i=0;i<n;i++)
    {
        b+=l[i];
    }
    return b;
}
int main()
{
    int n;
    cin>>n;
    int l[n];
    for(int i=0;i<n;i++)
    {
        cin>>l[i];
    }
    cout<<sum_up(n,l)<<endl;
    return 0;
}


2.
#include<iostream>
using namespace std;
int factorial(int n)
{
    int a=1;
    for(int i=1;i<=n;i++)
    {
        a*=i;
    }
    return a;
}
int main()
{
    int n;
    cin>>n;
    cout<<factorial(n)<<endl;
    return 0;
}


3.
#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;//length of the Fibonacci sequence
    int o[n];
    o[0]=1;o[1]=1;o[2]=2;
    for(int i=3;i<n;i++)
    {
        o[i]=o[i-2]+o[i-1];
    }
    for(int i=0;i<n;i++)
    {
        cout<<o[i]<<endl;
    }
}


4.
#include<iostream>
#include<algorithm>
#include<cmath>
using namespace std;
int main()
{
    int s=0.00000001;
    int a[3];
    for(int i=0;i<3;i++)
    {
        cin>>a[i];
    }
    sort(a,a+3);
    if(abs(pow(a[0],2)+pow(a[1],2)-pow(a[2],2))<s)
    {
        cout<<"Yes"<<endl;
    }
    else
    {
        cout<<"No"<<endl;
    }
    return 0;
}


