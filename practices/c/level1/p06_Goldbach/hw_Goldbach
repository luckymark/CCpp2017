#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);//max
    int l[n],m[n],f=0;
    for(int i=0;i<n-1;i++)
    {
        l[i]=i+2;
    }
    for(int i=0;i<n-1;i++)
    {
        if(l[i]!=0)
        {
            m[f]=l[i];
            f++;
            for(int j=2;j<=n/l[i];j++)
            {
                l[l[i]*j-2]=0;
            }
        }
    }
    for(int i=4;i<=n;i+=2)
    {
        for(int j=0;m[j]<=i/2;j++)
        {
            for(int k=j;m[j]+m[k]<=i and k<=n;k++)
            {
                if(m[j]+m[k]==i)
                {
                    printf("%d=%d+%d\n",i,m[j],m[k]);
                }
            }
        }
    }
    return 0;
}
